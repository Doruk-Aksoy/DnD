Actor ArmoredBipedalBall {
	PROJECTILE
	Speed 28
	Radius 4
	Height 8
	Damage (random(12, 18))
	Accuracy 133
	+NODAMAGETHRUST
	DamageType "Poison"
	Stamina 1073741952
