// For spawning from places
Actor DnD_FireJetProjectile {
    PROJECTILE
    Speed 25
    Height 16
    Radius 10
    Damage (25)
    DamageType "Trap"
    Stamina 16416 // Fire + Ignore Res
    +BRIGHT
    Scale 0.666
    Renderstyle Add
    Alpha 1.0
    SeeSound "Traps/Fireball"
    DeathSound "Traps/FireballHit"
    Species "Trap"
    States {
        Spawn:
            T000 AB 4
        Loop
        Death:
            TNT1 A 0 A_Explode(60, 80)
            T000 CDEFG 3 A_FadeOut(0.1)
        Stop
    }
}