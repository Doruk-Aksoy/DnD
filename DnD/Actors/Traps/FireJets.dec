Actor DnD_FireJetUp : DnD_SwitchableTrapBase 6663 {
    //$Category Traps
    Radius 8
    Height 60
    Scale 0.50
    +BRIGHT
    +NOGRAVITY
    +FLOORCLIP
    +NODAMAGETHRUST
    Obituary "%o was roasted by a fire jet."
    Stamina 16416 // Fire + Ignore Res

    const int dmg = 10;
    const int r = 34;
    
    States {
        Spawn:
        Inactive:
            T002 A 30
        Loop
        Active:
            T002 A 2 a_playsound("Traps/Firejet")
            T002 BCDEFGHIJKLMNOPQRSTU 2 A_explode(dmg, r, 0, 0, r)
            T002 VW 2
            T002 A 30
        Loop
    }
}

