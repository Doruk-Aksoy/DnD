Actor SpiderBossSpawner : DnD_MonsterSpawner replaces SpiderMastermind {
	//$Sprite SPIDA1D1
	Health 0x7FFFFFFF
	Height 100
	Radius 128
	-COUNTKILL
	-SOLID
	+NOCLIP
	+NOTDMATCH
	+BOSSDEATH
	States {
		Spawn:
			TNT1 A 0 NoDelay ACS_NamedExecuteAlways("DnD Monster Spawner", 0, DND_SPIDERMASTERMINDID)
		SpawnLoop:
			TNT1 A 1
		Loop
		Death:
			TNT1 A 5
			TNT1 A 1 A_BossDeath
		stop
		Vanilla:
			TNT1 A 0 A_SpawnItemEx("SpiderMastermind2", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Var1:
			TNT1 A 0 A_SpawnItemEx("Demolisher", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Var2:
			TNT1 A 0 A_SpawnItemEx("SpiderOverlord", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Var3:
			TNT1 A 0 A_SpawnItemEx("Arachnophyte", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Var4:
			TNT1 A 0 A_SpawnItemEx("PsionicQueen", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Var5:
			TNT1 A 0 A_SpawnItemEx("AngelofDeath", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Var6:
			TNT1 A 0 A_SpawnItemEx("GoldLich", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Var7:
			TNT1 A 0 A_SpawnItemEx("IronlichBoss", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Var8:
			TNT1 A 0 A_SpawnItemEx("DarkLich", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Unique:
			TNT1 A 0 A_SpawnItemEx("Eryxia", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		
		Incursion_Chex:
			TNT1 A 0 A_SpawnItemEx("Flembomination", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
		Incursion_Abyssal:
			TNT1 A 0 A_SpawnItemEx("AbyssalQueen", 0, 0, 0, 0, 0, 0, 0, SXF_NOCHECKPOSITION | SXF_TRANSFERAMBUSHFLAG | SXF_SETMASTER | SXF_TRANSFERSPECIAL | SXF_CLEARCALLERTID, 0, tid)
		Goto SpawnLoop
	}
}